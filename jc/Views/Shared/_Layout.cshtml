<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - jc</title>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
    <link rel="stylesheet" href="~/css/estilos.css">
    <script src="https://kit.fontawesome.com/ff59d8ddce.js" crossorigin="anonymous"></script>
    
</head>
<body>
    <header>
        <nav class="navbar">
            <div class="container-nav">
                <a class="logo"  asp-controller="Home" asp-action="Index">INVERSIONES JC</a>
                   
                    <ul class="nav-ul">
                        <li class="nav-item">
                            <a class="nav-link "  asp-controller="Home" asp-action="Index">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link "  asp-controller="Home" asp-action="Privacy">Nosotros</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                
            </div>
        </nav>
    </header>
    <section>
            @RenderBody()

    </section>
       

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - jc - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
